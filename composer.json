{
    "name": "im/primitive",
    "description": "PHP OOP Primitive Types",
    "license": "MIT",
    "keywords": ["type", "array", "string", "object", "int", "float", "bool", "strict typing", "object-oriented", "oop", "primitive", "laravel"],
    "authors": [
        {
            "name": "Igor Krimerman",
            "email": "i.m.krimerman@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5",
        "ext-mbstring": "*",
        "danielstjules/stringy": "~1.5",
        "firebase/php-jwt": "1.0.0",
        "symfony/var-dumper": "2.6.*"
    },
    "require-dev": {
        "phpspec/phpspec": "2.1.1",
        "henrikbjorn/phpspec-code-coverage": "1.0.1",
